
public class Patrat extends Figura{
	private double latura;
	
	public Patrat(){
		this.latura = 0;
	}
	
	public Patrat(double latura){
		this.latura = latura;
	}
	
	public double arie(){
		return latura*latura;
	}
	
	public double getLatura(){
		return this.latura;
	}
	
	public double perimetru(){
		return 4*latura;
	}
	
}
